{
   "defaultSeverity": "error",
   "extends": [
       "tslint:recommended"
   ],
   "jsRules": {},
   "rules": {
       "no-var-requires": false,
       "quotemark": [
           true,
           "single",
           "jsx-double"
       ],
       "interface-name": [
           false,
           "never-prefix"
       ],
       "radix": false
   }
}