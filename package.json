{
  "name": "v",
  "version": "0.0.0",
  "engines": {
    "node": "8.11.2"
  },
  "private": true,
  "scripts": {
    "dev:front": "cd client && npm run dev",
    "dev:server": "ts-node-dev --respawn --transpileOnly ./api/app.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:front\"",
    "tsc": "tsc",
    "clean": "rimraf ./build",
    "local:build": "tsc && cd client && npm run build",
    "start": "node build/app.js"
  },
  "dependencies": {
    "@material-ui/lab": "^4.0.0-alpha.31",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "express-jwt": "^5.3.1",
    "formidable": "^1.2.1",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^0.20.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.9",
    "morgan": "~1.9.1",
    "rimraf": "^2.6.3",
    "socket.io": "^2.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/formidable": "^1.0.31",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongodb": "^3.3.10",
    "@types/mongoose": "^5.5.28",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
